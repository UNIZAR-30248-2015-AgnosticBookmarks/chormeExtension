{
  "manifest_version": 2,
  "name": "Agnostic Bookmarks Chorme extension",
  "description": "Agnostic Bookmarks Chrome extension",
  "version": "1.0",

  "browser_action": {
    "default_icon": "img/icon.png",
    "default_popup": "public/popup.html"
