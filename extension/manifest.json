{
  "manifest_version": 2,
  "name": "Agnostic Bookmarks Chorme extension",
  "description": "Agnostic Bookmarks Chrome extension",
  "version": "1.0",

  "browser_action": {
    "default_icon": "img/icon.png",
    "default_popup": "public/popup.html"
  },
  "permissions": [
    "activeTab",
    "tabs",
	"storage",
	"http://*/*",
	"https://*/*"
  ],
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}